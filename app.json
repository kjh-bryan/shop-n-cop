{
  "expo": {
    "name": "shop-n-cop",
    "slug": "shop-n-cop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./frontend/assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./frontend/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7FFE5"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to have access to your camera.",
          "photosPermission": "Allow $(PRODUCT_NAME) to have access to your photos"
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./frontend/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kjhbryan.shopncop"
    },
    "web": {
      "favicon": "./frontend/assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "71f9bb6f-d277-4e8c-9600-06bbc3189a81"
      }
    }
  }
}
