{
  "expo": {
    "name": "shop-n-cop",
    "slug": "shop-n-cop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./frontend/assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./frontend/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7FFE5"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to have access to your camera.",
          "photosPermission": "Allow $(PRODUCT_NAME) to have access to your photos"
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "config": {
        "usesNonExemptEncryption": false
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.activate.shopncop",
      "runtimeVersion": {
        "policy": "sdkVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./frontend/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kjhbryan.shopncop",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./frontend/assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "340bbf9f-a5da-4181-835c-676e997d0670"
      }
    },
    "owner": "shop-n-cop",
    "updates": {
      "url": "https://u.expo.dev/340bbf9f-a5da-4181-835c-676e997d0670"
    }
  }
}
